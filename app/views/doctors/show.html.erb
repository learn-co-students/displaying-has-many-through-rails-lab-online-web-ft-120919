<h1>Doctor: <%= @doctor.name %></h1><br>
<h2><%= @doctor.department %></h2>
<br>


<ul><% @doctor.appointments.each do |appointment| %>
    <% @doctor_patient = @doctor.patients.find(appointment.patient_id) %>
  <li>  <%= link_to @doctor_patient.name, patient_path(@doctor_patient) %></li>
  <%= link_to appointment.appointment_datetime, appointment_path(appointment) %><br>    
  <% end %>
</ul>
